{
  "name": "web-ext-translator-online",
  "version": "3.1.0",
  "private": true,
  "homepage": ".",
  "bugs": {
    "url": "https://github.com/Lusito/web-ext-translator/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Lusito/web-ext-translator.git"
  },
  "license": "Zlib",
  "author": "Santo Pfingsten",
  "scripts": {
    "build": "rimraf docs .cache && tsc --noEmit && parcel build -d docs --public-url . src/index.html --no-source-maps",
    "lint": "npm run lint:es && npm run lint:css && npm run lint:package && npm run lint:style",
    "lint:css": "stylelint --ignore-path .prettierignore {src,wet-*/src}/**/*.css",
    "lint:css:fix": "stylelint --ignore-path .prettierignore --fix src/**/*.css",
    "lint:es": "eslint {src,wet-*/src}/**/*.{ts,tsx} --ext .ts,.tsx --ignore-path .prettierignore",
    "lint:es:fix": "npm run lint:es -- --fix",
    "lint:fix": "npm run lint:es:fix && npm run lint:css:fix && npm run lint:package:fix && npm run lint:style:fix",
    "lint:package": "npm run lint:package:fix -- --check",
    "lint:package:fix": "sort-package-json {.,wet-*}/package.json",
    "lint:style": "npm run lint:style:base -- --check",
    "lint:style:base": "prettier {src,wet-*/src}/**/*.{ts,tsx,js,json,css} {.,wet-*}/*.{ts,tsx,js,json,css}",
    "lint:style:fix": "npm run lint:style:base -- --write",
    "start": "parcel src/index.html"
  },
  "browserslist": [
    "> 5%"
  ],
  "dependencies": {
    "country-list": "^2.2.0",
    "editorconfig": "^0.15.3",
    "file-saver": "^2.0.2",
    "font-awesome": "^4.7.0",
    "iso-639": "^0.2.2",
    "iso-639-1": "^2.1.4",
    "jszip": "^3.5.0",
    "markdown-it": "^11.0.0",
    "minimatch": "^3.0.4",
    "murmurhash3js": "^3.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux-nano": "^1.1.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "throttle-debounce": "^2.3.0",
    "tsrux": "^1.2.0",
    "web-ext-translator-shared": "^3.1.0"
  },
  "devDependencies": {
    "@lusito/eslint-config-react": "^1.3.1",
    "@lusito/prettier-config": "^1.2.0",
    "@lusito/stylelint-config": "^1.2.0",
    "@types/file-saver": "^2.0.1",
    "@types/jszip": "^3.4.1",
    "@types/markdown-it": "10.0.2",
    "@types/minimatch": "^3.0.3",
    "@types/murmurhash3js": "^3.0.2",
    "@types/node": "^14.10.1",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/throttle-debounce": "^2.1.0",
    "parcel-bundler": "^1.12.4",
    "sort-package-json": "^1.44.0",
    "typescript": "^4.0.2"
  }
}
